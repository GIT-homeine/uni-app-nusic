<view class="Cloudvillage"><scroll-view class="bg-white nav text-center" scroll-x><block wx:for="{{topbar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',index==TabCur?'text-blue cur':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item.text+''}}</view></block></scroll-view><view class="cu-bar search bg-white"><view class="search-form round"><text class="cuIcon-search"></text><input adjust-position="{{false}}" type="text" placeholder="搜索图片、文章、视频" confirm-type="search" data-event-opts="{{[['focus',[['InputFocus',['$event']]]],['blur',[['InputBlur',['$event']]]]]}}" bindfocus="__e" bindblur="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['seach',['$event']]]]]}}" class="cu-btn bg-green shadow-blur round" bindtap="__e">搜索</button></view></view><block wx:if="{{isbarck==0}}"><view class="square"><view class="wrapper"><block wx:for="{{squarelist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['music',['$event']]]]]}}" class="list" bindtap="__e"><view class="img"><image src="{{item.url}}"></image></view><view class="name">{{item.text}}</view></view></block></view></view></block><block wx:else><block wx:if="{{isbarck==1}}"><view class="nearby">附近</view></block></block></view>